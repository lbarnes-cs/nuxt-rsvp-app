{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": [
      "@vueuse/core",
      "vue-i18n",
      "@nuxt/typescript",
      "vite-plugin-vuetify"
    ],
    "typeRoots": ["node_modules/@types", "types"],
    "baseUrl": ".",
    "paths": {
      "#app": ["./.nuxt/types/app.d.ts"],
      "@/*": ["./*"],
      "vuetify/*": ["node_modules/vuetify/lib/*"]
    }
  }
}
